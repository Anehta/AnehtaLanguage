// 测试点积 SIMD

print("=== Vec @ Vec (dot product, SIMD) ===")
var v1 = [1.0, 2.0, 3.0]
var v2 = [4.0, 5.0, 6.0]
var dot = v1 @ v2
print("v1 = ")
print(v1)
print("v2 = ")
print(v2)
print("v1 @ v2 = ")
print(dot)
// 期望: 1*4 + 2*5 + 3*6 = 4 + 10 + 18 = 32.0

print("")
print("=== Even length vectors ===")
var v3 = [1.0, 2.0, 3.0, 4.0]
var v4 = [5.0, 6.0, 7.0, 8.0]
var dot2 = v3 @ v4
print("v3 = ")
print(v3)
print("v4 = ")
print(v4)
print("v3 @ v4 = ")
print(dot2)
// 期望: 1*5 + 2*6 + 3*7 + 4*8 = 5 + 12 + 21 + 32 = 70.0
